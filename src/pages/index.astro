---
import Layout from '@/layouts/Layout.astro'
import Generator from '@/components/Generator.astro'
---

<Layout title="GitHub Profile Generator | GitHub Profile Readme Generator">
	<main>
		<!-- Banner -->
		<div class="mx-auto max-w-7xl px-4 sm:px-6 lg:px-8 pb-16 pt-20 text-center lg:pt-22">
			<h1 class="mx-auto max-w-4xl font-display text-5xl font-medium tracking-tight text-slate-900 sm:text-7xl">
				<span class="relative whitespace-nowrap text-blue-600">
					Awesome
				</span> GitHub Profile Generator
			</h1>
			<p class="mx-auto mt-6 max-w-2xl text-lg tracking-tight text-slate-700">Create awesome GitHub profiles with GitHub Profile Generator. Showcase your skills and projects easily with GitHub Profile Generator.</p>
		</div>

		<!-- Generator Form -->
		<Generator />
		<!-- FAQ -->
		<section class="pt-8 md:pt-12 lg:pt-20">
			<div class="container px-6 mx-auto text-center">
				<h2 class="text-2xl font-black sm:text-4xl md:text-4xl lg:text-5xl">Awesome GitHub Profile Readme Generator</h2>
